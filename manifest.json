{
	"manifest_version": 2,
	"name": "PopupTweaker",
	"description": "Popup Tweaker",
	"version": "1.2",
	"icons": {
		"16": "image.png",
		"32": "image.png",
		"48": "image.png",
		"64": "image.png",
		"128": "image.png"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content.js"
			]
		}
	]
}